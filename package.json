{
  "name": "diamondpkg",
  "version": "0.10.0",
  "description": "Dependency management built for Sass, Less, and CSS.",
  "preferGlobal": true,
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=4.0.0"
  },
  "scripts": {
    "depcheck": "depcheck --ignores=eslint,eslint-config-airbnb-base,eslint-plugin-import",
    "test": "npm run depcheck && eslint src/* && bash test/test.sh"
  },
  "bin": {
    "diamond": "src/cli/diamond.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diamondpkg/diamond.git"
  },
  "keywords": [
    "diamond",
    "package",
    "manager",
    "css",
    "sass",
    "scss",
    "less"
  ],
  "author": "Hackzzila",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diamondpkg/diamond/issues"
  },
  "homepage": "https://github.com/diamondpkg/diamond#readme",
  "dependencies": {
    "archy": "^1.0.0",
    "async": "^2.1.4",
    "chalk": "^1.1.3",
    "fs-extra": "^2.0.0",
    "fstream": "^1.0.11",
    "gonzales-pe": "^4.0.3",
    "ignore-file": "^1.1.2",
    "js-yaml": "^3.8.3",
    "klaw-sync": "^1.1.0",
    "less": "^2.7.2",
    "less-plugin-diamond": "^0.2.0",
    "node-sass": "^4.4.0",
    "npmlog": "^4.0.2",
    "os-name": "^2.0.1",
    "prompt": "^1.0.0",
    "proper-lockfile": "^2.0.0",
    "raven": "^1.2.1",
    "semver": "^5.3.0",
    "superagent": "^3.3.1",
    "tar": "^2.2.1",
    "tar-fs": "^1.15.2",
    "validate-npm-package-name": "^3.0.0",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "depcheck": "^0.6.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0"
  }
